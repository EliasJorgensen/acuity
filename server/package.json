{
  "name": "acuity-backend",
  "version": "0.1.0",
  "description": "Acuity REST Backend",
  "main": "index.js",
  "scripts": {
    "test": "node test/index.js | tap-diff",
    "test-ci": "nyc npm test && nyc report --reporter=lcov > coverage.lcov && codecov",
    "start": "node index",
    "db:create": "node util/db/create",
    "db:start": "docker start acuitydb",
    "db:stop": "docker stop acuitydb",
    "db:rm": "docker rm acuitydb"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Entake/acuity/"
  },
  "keywords": [
    "REST",
    "JavaScript",
    "thatimagetaggingsite",
    "Node",
    "RethinkDB",
    "GoogleVision"
  ],
  "author": "Elias JÃ¸rgensen <elias@entake.dk> (http://eliasjorgensen.me)",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-core": "^6.14.0",
    "babel-eslint": "^7.0.0",
    "babel-plugin-transform-do-expressions": "^6.8.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015-node6": "^0.3.0",
    "babel-preset-stage-1": "^6.16.0",
    "codecov": "^1.0.1",
    "eslint": "^3.6.1",
    "eslint-config-standard": "^6.1.0",
    "eslint-plugin-import": "^2.0.0",
    "eslint-plugin-promise": "^2.0.1",
    "eslint-plugin-standard": "^2.0.0",
    "nyc": "^8.3.1",
    "reqlite": "^2.3.0",
    "supertest": "^2.0.0",
    "supertest-koa-agent": "^0.3.0",
    "tap-diff": "^0.1.1",
    "tape": "^4.6.2"
  },
  "dependencies": {
    "jsonwebtoken": "^7.1.9",
    "kcors": "^2.2.0",
    "koa": "^2.0.0",
    "koa-bodyparser": "^3.2.0",
    "koa-convert": "^1.2.0",
    "koa-generic-session": "^1.11.3",
    "koa-morgan": "^1.0.1",
    "koa-passport": "^2.2.2",
    "koa-router": "^7.0.1",
    "passport": "^0.3.2",
    "passport-jwt": "^2.1.0",
    "passport-local": "^1.0.0",
    "thinky": "^2.3.6",
    "winston": "^2.2.0"
  },
  "babel": {
    "presets": [
      "es2015-node6",
      "stage-1"
    ],
    "plugins": [
      "transform-runtime",
      "transform-do-expressions"
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": [
      "standard"
    ],
    "rules": {
      "no-multi-spaces": [
        1,
        {
          "exceptions": {
            "VariableDeclarator": true,
            "ImportDeclaration": true
          }
        }
      ]
    }
  }
}
